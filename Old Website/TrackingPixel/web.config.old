<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.6.1">
      <assemblies>
        <add assembly="System.Device, Version=4.0.0.0, Culture=neutral, PublicKeyToken=B77A5C561934E089"/>
      </assemblies>
    </compilation>
    <httpRuntime targetFramework="4.6.1"/>
  </system.web>
    <system.webServer>
        <rewrite>
            <rules>
				<rule name="Rename Pixel" patternSyntax="Wildcard" stopProcessing="false">
                    <match url="*cleardot.gif" />
                    <action type="Rewrite" url="default.aspx" appendQueryString="false" logRewrittenUrl="true" />
                </rule>
                <rule name="Rename Pixel2" patternSyntax="Wildcard" stopProcessing="false">
                    <match url="*P3X2.gif" />
                    <action type="Rewrite" url="default_GeoTest.aspx" appendQueryString="false" logRewrittenUrl="true" />
                </rule>
                <rule name="Rename Pixel3" patternSyntax="Wildcard" stopProcessing="false">
                    <match url="*SMART.gif" />
                    <action type="Rewrite" url="default.aspx" appendQueryString="false" logRewrittenUrl="true" />
                </rule>
			</rules>
        </rewrite>
        <defaultDocument>
            <files>
                <clear />
                <add value="index.html" />
                <add value="index.php" />
                <add value="Default.htm" />
                <add value="Default.asp" />
                <add value="index.htm" />
                <add value="iisstart.htm" />
                <add value="default.aspx" />
            </files>
        </defaultDocument>
    </system.webServer>
</configuration>
